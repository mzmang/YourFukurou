{
  "name": "yourfukurou",
  "productName": "YourFukurou",
  "version": "0.0.0",
  "description": "Hackable YoruFukurou alternative",
  "main": "build/main.js",
  "bin": "./bin/cli.js",
  "scripts": {
    "dep": "npm install && typings install",
    "build-main": "tsc --pretty -p main",
    "build-renderer": "tsc --pretty -p renderer && browserify renderer/js -o build/renderer.js",
    "build": "npm run build-main && npm run build-renderer",
    "app": "./bin/cli.js",
    "debug": "NODE_ENV=development ./bin/cli.js",
    "dummy": "NODE_ENV=development YOURFUKUROU_DUMMY_TWEETS=true ./bin/cli.js",
    "watch": "guard --watchdir main renderer",
    "lint": "tslint $(git ls-files | grep -E \".tsx?$\")",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/YourFukurou.git"
  },
  "keywords": [
    "twitter client",
    "YoruFukurou",
    "Electron",
    "linux",
    "mac",
    "windows"
  ],
  "author": "rhysd <lin90162@yahoo.co.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/YourFukurou/issues"
  },
  "homepage": "https://github.com/rhysd/YourFukurou#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "electron-packager": "^6.0.0",
    "tslint": "^3.8.1",
    "typescript": "^1.8.10",
    "typings": "^0.7.12"
  },
  "dependencies": {
    "about-window": "^1.3.1",
    "animate.css": "^3.5.1",
    "dexie": "^1.3.6",
    "draft-js": "^0.5.0",
    "electron-prebuilt": "^0.37.7",
    "electron-window-state": "^2.0.0",
    "font-awesome": "^4.5.0",
    "immutable": "^3.7.6",
    "loglevel": "^1.4.0",
    "loglevel-message-prefix": "github:rhysd/loglevel-message-prefix",
    "node-emoji": "^1.3.0",
    "oauth": "github:rhysd/node-oauth",
    "object-assign": "^4.0.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-images": "^0.3.2",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "twit": "^2.2.4",
    "twitter": "^1.2.5",
    "twitter-text": "^1.13.4"
  }
}
